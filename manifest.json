         },

        {
         "addon": "personalizationaddon",
         "storefront": "smcustomstorefront",
         "template": "yacceleratorstorefront"
         },
    ],
         "aspects": [
        {
            "name": "backoffice",
            "webapps": [
                {
                    "name": "hac",
                    "contextPath": "/hac"
                },
                {
                    "name": "mediaweb",
