{
    "commerceSuiteVersion": "1905",
    "useCloudExtensionPack": true,    
    "extensions": [
        "modeltacceleratorservices",
        "electronicsstore",
        "yacceleratorstorefront",
        "backoffice",
        "smartedit",
        "cmssmartedit",
        "smarteditwebservices",
        "cmssmarteditwebservices",
        "oauth2",
        "cmswebservices",
        "permissionswebservices",
        "previewwebservices",
        "mediaweb"
    ],
    "storefrontAddons" : [
        {
         "addon": "smarteditaddon",
