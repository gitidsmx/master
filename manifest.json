                    {
                     “name”: “mediaweb”,
                     “contextPath”: “/medias”
                    },
                    {
                     "name": "personalizationsmartedit",
                     "contextPath": "/personalizationsmartedit"
                    },
                    {
                     "name": "personalizationpromotionssmartedit",
                     "contextPath": "/personalizationpromotionssmartedit"
